{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["google"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"indent": ["error", 4],
		"object-curly-spacing": ["error", "always"],
		"max-len": ["error", 140],
		"space-before-function-paren": ["error", "always"]
	}
}
